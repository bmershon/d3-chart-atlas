{
  "name": "d3.chart.atlas",
  "version": "0.1.0",
  "description": "A d3.chart for managing map projections.",
  "repository": {
    "type": "git",
    "url": "git://github.com/bmershon/d3-chart-atlas.git"
  },
  "keywords": [
    "d3.js",
    "visualization",
    "cartography",
    "map",
    "projection",
    "svg"
  ],
  "author": {
    "name": "Brooks Mershon"
  },
  "main": "atlas",
  "scripts": {
    "bundle": "d3-bundler --format=umd --name=atlas -- index.js > atlas.js",
    "minify": "uglifyjs atlas.js > atlas.min.js"
  },
  "devDependencies": {
    "d3-bundler": "~0.2.5",
    "uglify-js": "2.4.16"
  },
  "license": "MIT"
}
